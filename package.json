{
  "name": "AnticiPage",
  "description": "Predictive Routing for ExpressJs",
  "version": "0.0.1",
  "author": "Alid Castano <alidcastano@gmail.com>",
  "module": "dist/build.js",
  "main": "dist/build.js",
  "scripts": {
    "babel": "babel src --watch --out-dir build --plugins=transform-runtime --presets=es2015",
    "dev": "cross-env NODE_ENV=development node demo/server.js",
    "e2e": "cross-env NODE_ENV=development node test/e2e/runner.js",
    "unit": "cross-env NODE_ENV=development ./node_modules/.bin/mocha --reporter spec",
    "test": "npm run unit",
    "purge": "rm -rf dist/",
    "bundle": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "build": "npm run purge && npm run bundle",
    "upgrade": "npm --no-git-tag-version version patch && npm run build && npm publish"
  },
  "dependencies": {
    "pick-one-by-weight": "^1.0.2",
    "babel-register": "^6.24.1",
    "level": "^1.6.0",
    "markovchain": "^1.0.2",
    "sequelize": "^3.30.4",
    "sinon": "^1.17.7",
    "vue": "^2.1.8"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.0.0",
    "babel-eslint": "^7.1.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.2",
    "babel-loader": "^6.0.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.2.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-1": "^6.16.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chromedriver": "^2.26.1",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.16.1",
    "eslint-config-standard": "^7.0.0",
    "eslint-friendly-formatter": "^2.0.7",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-html": "^2.0.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "mocha": "^3.2.0",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.8.0",
    "vue-loader": "^10.0.0",
    "vue-template-compiler": "^2.1.8",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.1.0-beta.9",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-merge": "^1.1.2"
  }
}
